<%- include('../header') %>
  <!-- Content Start -->  
    <!-- Form Start -->
    <div class="container-fluid pt-4 px-4mt-5" >
        <div class="row g-4">
            <div class="col-sm-12 col-xl-6  mx-auto mt-5">
                <% if(locals.message?.success) {%>
                <div class="alert alert-success" role="alert">
                    <%= locals.message?.success %>
                  </div>
                  <% } %>
                <div class="bg-secondary rounded h-100 p-4 ">
                    <h4 align="center" style="font-weight: 500; letter-spacing: 0.19rem; " class="mb-4 ">Add Category</h4>
                    <form method="post" action="/category/insertcategory">
                        <div class="mb-3">
                            <label for="exampleInputEmail1" class="form-label">Category Name</label>
                            <input type="text" class="form-control" placeholder="Enter Category" name="category"aria-describedby="emailHelp" placeholder="Enter Category Name">
                        </div>
                        
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </form>
                </div>
            </div>
           
        </div>
    </div>
    <!-- Form End -->
<%- include('../footer') %>




